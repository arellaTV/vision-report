{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "no-console": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "guard-for-in": 0,
      "no-restricted-syntax": 0,
      "arrow-body-style": 0
    },
    "globals": {
      "document": 0,
      "fetch": 0,
      "describe": 0,
      "it": 0,
      "before": 0,
      "window": 0,
      "fetch": 0,
      "convertData": 0
    }
}